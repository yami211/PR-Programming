#include <stdio.h>

int countSequences(int n) {
    // Створення масиву dp для зберігання проміжних результатів
    int dp[n + 1];

    // Базові значення для обчислень
    dp[0] = 1;
    dp[1] = 2;
    dp[2] = 4;

    // Обчислення послідовностей для n > 2
    for (int i = 3; i <= n; i++) {
        // Формула рекурентного співвідношення для dp[i]
        dp[i] = (dp[i - 1] + dp[i - 2] + dp[i - 3]) % 12345;
    }

    // Повернення значення dp[n] як результат
    return dp[n];
}

int main() {
    int n;

    // Запит на введення довжини послідовності
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    // Виклик функції countSequences для обчислення кількості послідовностей
    int result = countSequences(n);

    // Виведення результату
    printf("Кількість шуканих послідовностей: %d\n", result);

    return 0;
}
